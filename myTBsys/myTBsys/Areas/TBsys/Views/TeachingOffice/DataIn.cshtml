
@{
    ViewBag.Title = "DataIn";
    Layout = "~/Areas/TBsys/Views/Shared/_TeachingOffice.cshtml";
}

<div class="panel panel-default">
    <div class="panel-heading">数据导入</div>
    <div class="panel-body">
        <ul class="list-group">

            <li class="list-group-item">
                <form action="/TBsys/TeachingOffice/TeacherIn" method="post">
                    <h4><strong>请添加教师信息的数据文件：</strong></h4>
                    <input type="file" name="fileName">
                    <p class="help-block">注：请添加格式为xls的Excel文件。</p>
                    <button type="submit" class="btn btn-default" onclick="result(this)">提交</button>
                </form>
            </li>

            <li class="list-group-item">
                <form action="/TBsys/TeachingOffice/StudentIn" method="post">
                    <h4><strong>请添加学生信息的数据文件：</strong></h4>
                    <input type="file" name="fileName">
                    <p class="help-block">注：请添加格式为xls的Excel文件。</p> 
                    <button type="submit" class="btn btn-default" onclick="result(this)">提交</button>
                </form>
            </li>

            <li class="list-group-item">
                <form action="/TBsys/TeachingOffice/TaskIn" method="post">
                    <h4><strong>请添加教学任务信息的数据文件：</strong></h4>
                    <input type="file" name="fileName">
                    <p class="help-block">注：请添加格式为xls的Excel文件。</p>
                    <button type="submit" class="btn btn-default" onclick="result(this)">提交</button>
                </form>

            </li>

        </ul>
    </div>
</div>
@section javascript{
    <script>
        function result(obj) {
            
                alert("导入成功！")
           
        }
    </script>
     
}






